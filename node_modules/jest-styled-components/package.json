{
  "name": "jest-styled-components",
  "version": "5.0.1",
  "description": "Jest utilities for Styled Components",
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "files": [
    "native",
    "src"
  ],
  "repository": "git@github.com:styled-components/jest-styled-components.git",
  "author": "Michele Bertoli",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "test": "yarn test:web && yarn test:native && yarn test:preact",
    "test:web": "jest",
    "test:native": "jest --config .jest.native.json",
    "test:preact": "jest --config .jest.preact.json"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^6.10.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "preact-render-to-json": "^3.6.6",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-native": "^0.53.3",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^3.2.0"
  },
  "dependencies": {
    "css": "^2.2.1"
  },
  "peerDependencies": {
    "styled-components": "^2.0.0 || ^3.0.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/__mocks__/requestAnimationFrame.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/native",
      "<rootDir>/test/preact"
    ]
  }
}
