{
  "name": "redux-seamless-immutable",
  "version": "0.4.0",
  "description": "Helpers for using seamless-immutable with Redux",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:watch": "watch 'npm run build' ./src/",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive ./test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "javascript",
    "redux",
    "immutable",
    "seamless",
    "router"
  ],
  "author": "Edward Stone <edwardjstone@yahoo.com>",
  "repository": "https://github.com/eadmundo/redux-seamless-immutable",
  "bugs": {
    "url": "https://github.com/eadmundo/redux-seamless-immutable/issues"
  },
  "homepage": "https://github.com/eadmundo/redux-seamless-immutable",
  "license": "MIT",
  "dependencies": {
    "react-router-redux": "^4.0.0",
    "seamless-immutable": "^7.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "eslint": "~2.2.0",
    "html-webpack-plugin": "^2.14.0",
    "mocha": "^2.4.5",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "rimraf": "^2.5.2",
    "watch": "^0.17.1",
    "webpack": "^1.12.14",
    "webpack-hot-middleware": "^2.10.0"
  },
  "npmName": "redux-seamless-immutable",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
}
