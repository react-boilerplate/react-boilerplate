{
  "name": "postcss-sass",
  "version": "0.3.2",
  "description": "A Sass parser for PostCSS, using gonzales-pe.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-sass",
    "sass"
  ],
  "author": "Oleh Aloshkin <olegaleshkin@gmail.com>",
  "license": "MIT",
  "repository": "AleshaOleg/postcss-sass",
  "bugs": {
    "url": "https://github.com/AleshaOleg/postcss-sass/issues"
  },
  "homepage": "https://github.com/AleshaOleg/postcss-sass",
  "dependencies": {
    "gonzales-pe": "4.2.3",
    "postcss": "6.0.22"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-precompile-charcodes": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-postcss": "^2.0.2",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jest": "^22.4.3",
    "postcss-parser-tests": "^6.2.1",
    "postcss-scss": "^1.0.5"
  },
  "scripts": {
    "precommit": "npm test",
    "prepack": "npm run build",
    "clean": "rm *.js || echo 'Already cleaned'",
    "build": "npm run clean && babel -s inline -d ./ *.es6",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "prepublishOnly": "npm run build",
    "pretest": "eslint *.es6 __tests__",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss",
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": [
        "error",
        120
      ],
      "prefer-const": "error"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/**/*"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "es6",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.es6$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "last 1 version",
            "node": 4
          },
          "loose": true
        }
      ]
    ],
    "plugins": [
      "add-module-exports",
      "precompile-charcodes"
    ]
  },
  "main": "index.js"
}
