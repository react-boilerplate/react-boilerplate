    "rollup-plugin-commonjs": "^2.2.0",
    "rollup-plugin-memory": "^1.0.0",
    "rollup-plugin-npm": "^1.3.0",
    "rollup-plugin-replace": "^1.1.0",
    "rollup-plugin-uglify": "^0.1.0",
    "sauce-tunnel": "^2.2.3",
    "serve-static": "^1.10.0",
    "wd": "^0.3.6"
