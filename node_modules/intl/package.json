    "cldr-cal-persian-full": "28.0.0",
    "cldr-cal-roc-full": "28.0.0",
    "cldr-core": "28.0.0",
    "cldr-dates-full": "28.0.0",
    "cldr-numbers-full": "28.0.0",
    "cli-color": "^1.0.0",
    "clui": "^0.3.1",
    "eslint": "^2.2.0",
    "estraverse-fb": "^1.3.1",
    "finalhandler": "^0.4.0",
    "glob": "^5.0.3",
    "grunt": "^0.4.5",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.5.0",
