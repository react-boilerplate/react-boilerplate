{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=12.16.1"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "npmcheckupdates": "npx nmp-check-updates",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest:coverage": "npm run test:clean",
    "test:clean": "rm -f ./coverage-summary.json",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --silent --coverage --coverageDirectory='./' --coverageReporters=\"json-summary\"",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@formatjs/intl-getcanonicallocales": "^1.5.3",
    "@formatjs/intl-locale": "^2.4.14",
    "@formatjs/intl-pluralrules": "^4.0.6",
    "@formatjs/intl-relativetimeformat": "^8.0.4",
    "chalk": "^4.1.0",
    "compression": "1.7.4",
    "connected-react-router": "6.8.0",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "fontfaceobserver": "2.1.0",
    "history": "^4.10.1",
    "hoist-non-react-statics": "3.3.2",
    "immer": "8.0.1",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "lodash": "^4.17.20",
    "memfs": "^3.2.0",
    "minimist": "^1.2.5",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-intl": "5.12.3",
    "react-redux": "7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "sanitize.css": "12.0.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.12.16",
    "@babel/core": "7.12.16",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.12.13",
    "@babel/plugin-transform-react-constant-elements": "7.12.13",
    "@babel/plugin-transform-react-inline-elements": "7.12.13",
    "@babel/preset-env": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "@babel/register": "7.12.13",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-styled-components": "1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "5.2.2",
    "compare-versions": "3.6.0",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "5.0.2",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-redux-saga": "1.2.1",
    "file-loader": "6.2.0",
    "html-loader": "2.0.0",
    "html-webpack-plugin": "5.1.0",
    "image-webpack-loader": "7.0.1",
    "imports-loader": "2.0.0",
    "jest-cli": "^26.6.3",
    "jest-dom": "3.1.3",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.4",
    "ngrok": "3.4.0",
    "node-plop": "0.26.2",
    "npm-check-updates": "^11.1.4",
    "null-loader": "4.0.1",
    "offline-plugin": "5.0.7",
    "plop": "2.7.4",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "react-app-polyfill": "2.0.0",
    "react-test-renderer": "17.0.1",
    "react-testing-library": "6.1.2",
    "rimraf": "3.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "2.0.0",
    "stylelint": "^13.10.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "4.1.1",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "4.1.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-pwa-manifest": "^4.3.0",
    "whatwg-fetch": "3.5.0"
  }
}
